{
  "name": "ledgetopdroneservices/project-alpha",
  "description": "Project Alpha API / web app",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-pdo": "*",
    "ext-mbstring": "*",
    "phpmailer/phpmailer": "^6.9",
    "dompdf/dompdf": "^2.0",
    "symfony/security-csrf": "^7.0",
    "symfony/http-foundation": "^7.0",
    "twig/twig": "^3.21"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
"autoload": {
  "psr-4": {
    "App\\": "src/"
  },
  "exclude-from-classmap": [
    "vendor/dompdf/dompdf/lib/"
  ]
},
  "scripts": {
    "test": "phpunit --colors=always",
    "start": "php -S 0.0.0.0:8080 -t public"
  }
}
